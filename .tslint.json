{
	"lintOptions": {"typeCheck": true},
	"rules": {
		"cyclomatic-complexity": true,
		"newline-before-return": false,
		"no-parameter-properties": false,
		"no-parameter-reassignment": false,
		"no-unused-variable": false,
		"no-var-requires": false,
		"no-irregular-whitespace": true,
		"no-trailing-whitespace": true,
		"arrow-return-shorthand": true,
		"prefer-const": true,
		"indent": [true, "spaces", 4],
		"semicolon": [true, "never"],
		"align": {
			"options": [
				"parameters",
				"statements",
				"members",
				"elements"
			]
		},
		"member-access": true,
		"only-arrow-functions": [true, "allow-declarations"],
		"no-empty": [true, "allow-empty-catch"],
		"completed-docs": false,
		"jsdoc-format": true,
		"encoding": true,
		"no-any": false,
		"no-magic-numbers": false,
		"no-non-null-assertion": false,
		"no-null-keyword": false,
		"no-require-imports": false,
		"no-unbound-method": false,
		"no-use-before-declare": false,
		"no-void-expression": false,
		"strict-type-predicates": false,
		"triple-equals": true,
		"typedef": false,
		"ban": [true, ["describe", "only"], ["it", "only"]],
		"interface-name": false,
		"comment-format": [true, "check-space"],
		"max-classes-per-file": false,
		"prefer-function-over-method": [true, "allow-public", "allow-protected"],
		"no-unnecessary-qualifier": true,
		"object-literal-key-quotes": [true, "as-needed"],
		"quotemark": [true, "double", "avoid-escape", "avoid-template"],
		"whitespace": [
			true,
			"check-branch",
			"check-operator",
			"check-typecast",
			"check-separator",
			"check-type",
			"check-decl",
			"check-module"
	],
	"max-line-length": [true, 140],
	"class-name": true,
	"member-ordering": [true,
		"public-before-private",
		"static-before-instance",
		"variables-before-functions"
	],
	"no-console": false,
	"no-switch-case-fall-through": true,
	"strict-boolean-expressions": [true, "allow-boolean-or-undefined"],
	"switch-default": false,
	"variable-name": [true,
		"ban-keywords",
		"check-format",
		"allow-leading-underscore",
		"allow-pascal-case"
	]
	}
}
