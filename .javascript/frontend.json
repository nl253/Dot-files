{
  "name": "Frontend",
  "version": "0.0.1-alpha",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "read": "cd docs && python3 -m http.server",
    "docs": "node_modules/.bin/jsdoc --verbose --destination docs/ --recurse $(find src/ -type f -and -not -path '*node_modules/*' -and -not -path '*.git/*' -and -not -path '*docs/*' -and -not -path '*.idea/*' -and -not -path '*dist/*' -and -name '*.js')",
    "test": "node_modules/.bin/jest"
  },
  "dependencies": {
    "react": "*",
    "react-dom": "*",
    "react-scripts": "*"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "eslint-config-frontend-react": "https://github.com/nl253/eslint-config-frontend-react",
    "jest": "^21.0.2",
    "js-beautify": "*",
    "jsdoc": "*"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "eslintConfig": {
    "extends": "eslint-config-frontend-react"
  }
}
