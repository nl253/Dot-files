--langdef=Haskell
--langmap=Haskell:.hs 
--regex-Haskell=/^\s*([a-z]\w+)\s+::\s+.*/\1/f,function/
--regex-Haskell=/^module\s+([A-Z]\w+)/\1/m,module/
--regex-Haskell=/^\s*class\s+([A-Z]\w+)/\1/c,class/
--regex-Haskell=/^\s*data\s+([A-Z]\w+)/\1/d,data/
--regex-Haskell=/^\s*(new)?type\s+([A-Z]\w+)/\2/t,type alias/
