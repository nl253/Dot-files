--langdef=MyRust 
--langmap=MyRust:.rs
--regex-MyRust=/^\s*pub\s+fn\s+(\w+)/\1/f,function/
--regex-MyRust=/^\s*pub\s+struct\s+(\w+)/\1/C,struct/
--regex-MyRust=/^\s*pub\s+union\s+(\w+)/\1/U,union/
--regex-MyRust=/^\s*pub\s+trait\s+(\w+)/\1/I,trait/
--regex-MyRust=/^\s*pub\s+const\s+(\w+)/\1/c,const/
--regex-MyRust=/^\s*pub\s+type\s+(\w+)/\1/a,type alias/
--regex-MyRust=/^\s*pub\s+enum\s+(\w+)/\1/E,enum/
--regex-MyRust=/^\s*extern\s+crate\s+(\w+)/\1/e,extern crate/
